{
    "extensions.ignoreRecommendations": true,
    "telemetry.telemetryLevel": "error",
    "editor.accessibilitySupport": "off",
    "editor.codeActionsOnSave": {
        "source.organizeImports": true
    },
    "editor.codeLens": false,
    "editor.comments.insertSpace": false,
    "editor.copyWithSyntaxHighlighting": false,
    "editor.fontLigatures": true,
    "editor.dragAndDrop": false,
    "editor.fontFamily": "JetBrains Mono",
    "editor.fontSize": 13,
    "editor.formatOnSave": true,
    "editor.links": false,
    "editor.maxTokenizationLineLength": 2000,
    "editor.minimap.enabled": false,
    "editor.semanticHighlighting.enabled": true,
    "explorer.confirmDelete": false,
    "files.hotExit": "off",
    "files.insertFinalNewline": true,
    "files.participants.timeout": 0,
    "files.restoreUndoStack": false,
    "gopls": {
        "build.directoryFilters": [
            "-node_modules",
            "-.vscode",
        ],
        "ui.diagnostic.annotations": {
            "inline": true,
            "bounds": true,
            "nil": true,
            "escape": true,
        },
        "build.experimentalPackageCacheKey": true,
        "build.allowModfileModifications": true,
        "ui.diagnostic.analyses": {
            "asmdecl": false,
            "assign": true,
            "atomic": false,
            "atomicalign": false,
            "bools": true,
            "buildtag": true,
            "cgocall": false,
            "composites": true,
            "copylocks": true,
            "deepequalerrors": true,
            "embed": true,
            "errorsas": true,
            "fieldalignment": true,
            "httpresponse": true,
            "ifaceassert": true,
            "infertypeargs": true,
            "loopclosure": true,
            "lostcancel": true,
            "nilfunc": true,
            "nilness": true,
            "printf": true,
            "shadow": true,
            "shift": true,
            "simplifycompositelit": true,
            "simplifyrange": true,
            "simplifyslice": true,
            "sortslice": true,
            "stdmethods": true,
            "stringintconv": true,
            "structtag": true,
            "testinggoroutine": false,
            "tests": true,
            "unmarshal": true,
            "unreachable": true,
            "unsafeptr": false,
            "unusedparams": true,
            "unusedresult": true,
            "unusedwrite": true,
            "useany": true,
            "fillreturns": true,
            "nonewvars": true,
            "noresultvalues": true,
            "undeclaredname": true,
            "fillstruct": true,
            "stubmethods": true,
        },
        "formatting.gofumpt": true,
        "ui.semanticTokens": true,
        "ui.completion.usePlaceholders": true,
        "ui.completion.experimentalPostfixCompletions": true,
        //"build.expandWorkspaceToModule": true,
        //"build.experimentalWorkspaceModule": true,
        //"ui.diagnostic.experimentalWatchedFileDelay": "5s",
        "ui.completion.matcher": "Fuzzy",
    },
    "[go]": {
        "files.trimTrailingWhitespace": false,
        "files.trimFinalNewlines": false,
    },
    "go.testFlags": [
        "-count",
        "1",
    ],
    "go.disableConcurrentTests": true,
    "go.inlayHints.compositeLiteralFields": true,
    "go.inlayHints.parameterNames": true,
    "go.inlayHints.rangeVariableTypes": true,
    "security.workspace.trust.enabled": false,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.customGlyphs": false,
    "terminal.integrated.fontSize": 12,
    "workbench.editor.enablePreview": false,
    "workbench.startupEditor": "none",
    "workbench.welcomePage.walkthroughs.openOnInstall": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.autoIndent": "advanced",
    "editor.occurrencesHighlight": false,
    "editor.selectionHighlight": false,
    "blade.format.enable": true,
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "intelephense.diagnostics.run": "onSave",
    "intelephense.environment.phpVersion": "8.1.4",
    "intelephense.format.enable": false,
    "explorer.autoReveal": false,
    "clangd.fallbackFlags": [
        "-std=c11"
    ],
    "editor.formatOnSaveMode": "modificationsIfAvailable",
    "html.format.endWithNewline": true,
    "php-cs-fixer.formatHtml": true,
    "php-cs-fixer.lastDownload": 0,
    "php-cs-fixer.onsave": true,
    "php.validate.executablePath": "/usr/bin/php",
    "php-cs-fixer.allowRisky": true,
    "php-cs-fixer.executablePath": "php-cs-fixer",
    "workbench.tree.expandMode": "doubleClick",
    "editor.multiCursorModifier": "ctrlCmd",
    "svelte.plugin.svelte.format.config.singleQuote": true,
    "svelte.plugin.svelte.format.config.svelteStrictMode": true,
    "svelte.enable-ts-plugin": true,
    "php.suggest.basic": false,
    "intelephense.phpdoc.returnVoid": false,
    "intelephense.phpdoc.textFormat": "text",
    "html.format.templating": true,
    "nginx-conf-hint.syntax": "original",
    "redhat.telemetry.enabled": false,
    "security.workspace.trust.startupPrompt": "never",
    "security.workspace.trust.untrustedFiles": "open",
    "typescript.workspaceSymbols.scope": "currentProject",
    "workbench.colorTheme": "Gruvbox Dark Hard",
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "editor.guides.bracketPairs": "active",
    "editor.bracketPairColorization.enabled": false,
    "better-comments.tags": [
        {
            "tag": "!",
            "color": "#c27a7a",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "?",
            "color": "#7b7ac2",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "//",
            "color": "#474747",
            "strikethrough": true,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "@",
            "color": "#c27a99",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "todo",
            "color": "#b5c27a",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "fixme",
            "color": "#c2ab7a",
            "strikethrough": false,
            "underline": true,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "bug",
            "color": "#c2827a",
            "strikethrough": false,
            "underline": true,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": false
        },
        {
            "tag": "[ ]",
            "color": "#7aacc2",
            "strikethrough": false,
            "underline": false,
